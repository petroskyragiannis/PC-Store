<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pcstore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.pcstore.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pcstore.RegisterActivity">
        </activity>
        <activity android:name="com.example.pcstore.CatalogActivity">
        </activity>
        <activity android:name="com.example.pcstore.ConfigurationActivity">
        </activity>
        <activity android:name="com.example.pcstore.ComponentActivity">
        </activity>
        <activity android:name="com.example.pcstore.CartActivity">
        </activity>
    </application>

</manifest>